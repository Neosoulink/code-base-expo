{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"paths": {
			"@/*": ["./src/*"],
			"@components/*": ["./src/components/*"],
			"@helpers/*": ["./src/helpers/*"],
			"@store/*": ["./src/store/*"],
			"@imgs/*": ["./src/assets/img/*"],
			"@fonts/*": ["./src/assets/fonts/*"],
			"@hooks": ["./src/hooks.ts"],
			"@styles": ["./src/assets/ts/styles.ts"]
		}
	}
}
